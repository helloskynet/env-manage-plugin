{
  "name": "env-manage-plugin",
  "version": "0.4.9",
  "description": "A dev env plugin that integrates an Express server with request proxying capabilities.",
  "keywords": [
    "proxy",
    "devServer",
    "Development environment management",
    "webpack-plugin",
    "rollup-plugin",
    "vite-plugin"
  ],
  "bin": {
    "envm": "bin/index.js"
  },
  "main": "./dist/Plugin.js",
  "types": "./dist/types.d.ts",
  "exports": {
    ".": {
      "import": "./dist/Plugin.js",
      "types": "./dist/types.d.ts"
    }
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "pnpm -r --parallel dev",
    "dev:server": "node test/server.js",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "prepare": "husky"
  },
  "author": "lcn",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^17.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@eslint/json": "^0.12.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.16.4",
    "eslint": "^9.27.0",
    "husky": "^9.1.7",
    "nodemon": "^3.1.9",
    "typescript-eslint": "^8.37.0"
  },
  "bugs": {
    "url": "https://github.com/helloskynet/env-manage-plugin/issues"
  },
  "homepage": "https://github.com/helloskynet/env-manage-plugin#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/helloskynet/env-manage-plugin.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "bin",
    "dist",
    "templates",
    "README.md",
    "LICENSE"
  ]
}
