{
  "main": "src/index.js",
  "bin": {
    "envmanage": "bin/index.js"
  },
  "scripts": {
    "dev": "concurrently --prefix \"{name}\" --names \"Proxy,Client,TestServer\" \"npm run dev:proxy\" \"npm run dev:client\" \"npm run dev:server\"",
    "dev:proxy": "nodemon bin/index.js --config=env.config.js",
    "dev:client": "cd client && npm run dev",
    "dev:server": "node test/server.js",
    "build:client": "cd client && npm run build",
    "start": "node src/index.js"
  },
  "dependencies": {
    "chokidar": "^4.0.3",
    "express": "^4.21.2",
    "http-proxy-middleware": "^3.0.3",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9"
  }
}
